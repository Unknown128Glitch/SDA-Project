    if(us == 1){
        System.out.println("you are user");
            Manager ma = new Manager();
            ma.Manage();
    }
    
    else if(us == 2){
        NullPatternDemo n = new NullPatternDemo();
       
        boolean a = n.NPD();
      
        if(a == true){
            System.out.println("you are not admin");
        }
        else if(a == false){
         System.out.println("you are  admin");
         
        }
    
   
    }
    }
